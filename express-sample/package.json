    "fs": "^0.0.1-security",
    "morgan": "^1.8.1",
    "node-sass-middleware": "^0.11.0",
    "pug": "^2.0.0-rc.1",
    "request": "^2.81.0",
    "serve-favicon": "^2.4.2"
  },
  "devDependencies": {
    "chai": "^4.1.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.5.0",
    "mocha-sinon": "^2.0.0",
    "nyc": "^11.1.0",
    "should": "^11.2.1",
    "sinon": "^3.2.0",
    "supertest": "^3.0.0"
