  "main": "index.js",
  "license": "MIT",
  "nyc": {
    "check-coverage": true,
    "include": [
      "app/**/*.js"
    ],
    "exclude": [
      "app/spec/**/*.spec.js",
      "app/config",
      "app/coverage",
      "app/log",
      "app/public"
